<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="xyz.vaith.app.domain.BaseDict" table="base_dict">
        <id name="did" column="dict_id">
            <generator class="uuid"></generator>
        </id>
        <property name="typeCode" column="dict_type_code" />
        <property name="typeName" column="dict_type_name" />
        <property name="itemCode" column="dict_item_code" />
        <property name="itemName" column="dict_item_name" />
        <property name="sort" column="dict_sort" />
        <property name="enable" column="dict_enable" />
        <property name="memo" column="dict_memo" />
    </class>
</hibernate-mapping>